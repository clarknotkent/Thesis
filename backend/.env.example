# Server
PORT=3000

# JWT used by the application (not Supabase)
JWT_SECRET=replace_with_strong_secret

# Supabase project URL
SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co

# Server-side key: prefer SUPABASE_SERVICE_KEY (service_role)
# DO NOT commit real keys. Copy this file to .env and fill in values.
SUPABASE_SERVICE_KEY=your_service_role_key

# Optional fallback (anon key). Not recommended for server use.
# Only used if SUPABASE_SERVICE_KEY is not set.
SUPABASE_KEY=your_anon_key_optional
